{
  "name": "cordova-code-swap",
  "version": "0.0.2",
  "description": "Allows fetching updates to js-part of app",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbjorge/cordova-code-swap.git"
  },
  "scripts": {
    "build": "run-s build:clean build:js",
    "build:js": "babel src --out-dir build --plugins transform-object-assign --presets=es2015",
    "build:clean": "mkdir -p build && rm -r build/*",
    "test": "node test.js",
    "test:ci": "nodemon --exec 'node test.js'",
    "lint": "eslint src/*"
  },
  "keywords": [],
  "author": "Joachim Bj√∏rge <jb@ground.no> (http://ground.no)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2"
  }
}
